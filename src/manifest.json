{
  "manifest_version": 2,
  "minimum_chrome_version": "49",
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "./background.bundle.js"
    ]
  },
  "web_accessible_resources": []
}
